archetype extension transferable (

  variable[%transferable] aRole role

) = {

  variable <%aRole>_tmp role

  action assign_new_<%aRole> (newrole : <%aRole>tmp) = {
    called by aRole
    effect
      <%aRole>_tmp := newrole
  }

  action confirm_<%aRole> = {
    called by <%aRole>tmp
    effect
      aRole := <%aRole>tmp
  }
}
