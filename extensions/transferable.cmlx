model extension transferable (

  variable[%transferable] aRole role

) = {

  variable aRole role

  variable <%aRole>_tmp role

  transaction assign_new_<%aRole> (newrole : <%aRole>tmp) = {
    called by aRole
    action
      <%aRole>_tmp := newrole
  }

  transaction confirm_<%aRole> = {
    called by <%aRole>tmp
    action
      aRole := <%aRole>tmp
  }
}
