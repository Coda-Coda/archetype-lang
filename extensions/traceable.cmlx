archetype extension traceable (

  variable fromP role

  variable toP role

  variable[%traceable] anamount tez from fromP to toP
) = {

  variable fromP role

  variable toP role

  variable anamount tez from participant to toP

  action transfer_<%anamount> = {
    called by fromP
    condition
      c1 : transferred = anamount
  }
}
