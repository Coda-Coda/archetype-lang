archetype test_list_mynth2

variable res : string = ""

function mynth(l : list<string>, idx : nat) : string {
  var r : option<string> = none;
  var i : nat = 0;
  while i < idx do
    var e = "";
    r := some(e);
  done;
  return opt_get(r)
}

entry exec () {
  specification {
    s0: res = "2";
  }
  effect {
    var l : list<string> = ["1"; "2"; "3"];
    res := mynth(l, 1)
  }
}
