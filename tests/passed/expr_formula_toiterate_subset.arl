archetype expr_formula_toiterate_subset

asset o_asset identified by oid {
  oid : int;
}

asset my_asset identified by id {
  id : string;
  s : o_asset subset;
}

entry exec () {
  specification {
    postcondition p1 {
      true
      invariant for loop {
         toiterate.isempty()
      }
    }
  }

  effect {
    var res : int = 0;
    for:loop i in my_asset["0"].s do
      res += 1;
    done
  }
}
