archetype effect_method_asset_clear_subset

asset o_asset identified by oid {
  oid : string;
  val : int = 0;
}

asset my_asset identified by id {
  id     : string;
  value  : int;
  col    : o_asset subset;
} initialized by {
  {"id0"; 0; []};
  {"id1"; 1; []};
  {"id2"; 2; []}
}

entry exec () {
  effect {
    my_asset["id0"].col.removeif(the.val = 0)
  }
}
